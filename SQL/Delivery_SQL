-- Table: public.delivery

-- DROP TABLE public.delivery;

CREATE TABLE public.delivery
(
    delivery_id integer NOT NULL,
    delivery_company text COLLATE pg_catalog."default" NOT NULL,
    deliverer_address text COLLATE pg_catalog."default" NOT NULL,
    deliverable boolean,
    CONSTRAINT delivery_pkey PRIMARY KEY (delivery_id)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.delivery
    OWNER to postgres;
