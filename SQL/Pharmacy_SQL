-- Table: public.pharmacy

-- DROP TABLE public.pharmacy;

CREATE TABLE public.pharmacy
(
    pharm_id integer NOT NULL,
    pharm_name text COLLATE pg_catalog."default" NOT NULL,
    pharm_address text COLLATE pg_catalog."default" NOT NULL,
    CONSTRAINT pharmacy_pkey PRIMARY KEY (pharm_id)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.pharmacy
    OWNER to postgres;

COMMENT ON COLUMN public.pharmacy.pharm_name
    IS 'Name of pharmacy';

COMMENT ON COLUMN public.pharmacy.pharm_address
    IS 'Address to a pharmacy';
