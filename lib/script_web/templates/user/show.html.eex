<h2>User</h2>

<ul>

  <li>
    <strong>User Name:</strong>
    <%= @user.user_name %>
  </li>
  <li>
    <strong>User Type:</strong>
    <%= @user.user_type %>
  </li>
  <li>
    <strong>Company:</strong>
    <%= @user.company_id %>
  </li>
</ul>

<span><%= link "Edit", to: user_path(@conn, :edit, @user) %></span>
<div class="row back">
  <a class="col-sm-2 col-sm-offset-5 btn text-center" id="go_back" href="/">Go Back</a>
</div>
